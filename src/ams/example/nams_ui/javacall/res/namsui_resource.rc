#include "windows.h"                 /* Windows stuff */
#include "namsui_resource.h"         /* Program-specific stuff */

STRINGTABLE DISCARDABLE
BEGIN
    IDS_EMPTY, ""
END

ID_MENU_MAIN MENU
BEGIN
    POPUP "Suite..."
    BEGIN
        MENUITEM "Info...", IDM_SUITE_INFO
        MENUITEM "Settings...", IDM_SUITE_SETTINGS
        MENUITEM SEPARATOR
        MENUITEM "Remove", IDM_SUITE_REMOVE
        MENUITEM "Update", IDM_SUITE_UPDATE
        MENUITEM SEPARATOR
        MENUITEM "Exit", IDM_SUITE_EXIT
    END

    POPUP "MIDlet..."
    BEGIN
        MENUITEM "Launch", IDM_MIDLET_START_STOP
        MENUITEM "To Foreground", IDM_MIDLET_TO_FOREGROUND
        MENUITEM "Info...", IDM_MIDLET_INFO
        MENUITEM SEPARATOR
        MENUITEM "Show Running...", IDM_MIDLET_SHOW_RUNNING
    END

    POPUP "Help"
    BEGIN
        MENUITEM "About...", IDM_HELP_ABOUT
    END
END

ID_MENU_POPUP_MIDLET MENU DISCARDABLE
BEGIN
    POPUP "MIDletPopupMenu"
    BEGIN
        MENUITEM "Launch", IDM_MIDLET_START_STOP
        MENUITEM "Info...", IDM_MIDLET_INFO
        MENUITEM "Remove", IDM_MIDLET_REMOVE
        MENUITEM "Update", IDM_MIDLET_UPDATE
        MENUITEM "Settings...", IDM_MIDLET_SETTINGS
    END
END

