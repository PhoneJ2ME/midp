<configuration>
<constants>

<constant_class Package="com.sun.midp.configurator" Name="AlertTypeConstants"
    Scope="public" Comment="/** Constants for alert types */">
  <constant Type="int"
            Name="LCDUI_ALERT_TYPE_INFO"
            Value="1"
            Comment="Information Alert"/>
  <constant Type="int"
            Name="LCDUI_ALERT_TYPE_WARNING"
            Value="2"
            Comment="Warning Alert"/>
  <constant Type="int"
            Name="LCDUI_ALERT_TYPE_ERROR"
            Value="3"
            Comment="Error Alert"/>
  <constant Type="int"
            Name="LCDUI_ALERT_TYPE_ALARM"
            Value="4"
            Comment="Alarm Alert"/>
  <constant Type="int"
            Name="LCDUI_ALERT_TYPE_CONFIRMATION"
            Value="5"
            Comment="Confirmation Alert"/>
</constant_class>

</constants>
</configuration>
